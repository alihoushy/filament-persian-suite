{
    "name": "alihoushy/filament-persian-suite",
    "description": "Filament Persian Suite - Comprehensive Persian localization for FilamentPHP v4",
    "type": "library",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "filamentphp",
        "filament",
        "persian",
        "rtl",
        "jalali"
    ],
    "license": "MIT",
    "homepage": "https://github.com/alihoushy/filament-persian-suite",
    "support": {
        "issues": "https://github.com/alihoushy/filament-persian-suite/issues",
        "source": "https://github.com/alihoushy/filament-persian-suite"
    },
    "authors": [
        {
            "name": "Ali Houshyari Pour",
            "email": "alihoushyaripour@gmail.com",
            "homepage": "https://github.com/alihoushy"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "filament/filament": "^4.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Alihoushy\\FilamentPersianSuite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alihoushy\\FilamentPersianSuite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alihoushy\\FilamentPersianSuite\\FilamentPersianSuiteServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
